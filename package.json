{
  "name": "worker",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc",
    "dev": "tsx watch src/server.ts",
    "dev:build": "npm run build && npm run dev",
    "start": "npm run build && node dist/server.js",
    "create-new-queue": "tsx src/shared/queues/create-new-queue.ts",
    "migrate": "tsx node_modules/.bin/knex --knexfile src/shared/database/config.ts migrate:latest",
    "migrate:rollback": "tsx src/shared/database/list-last-batch-migrations.ts"
  },
  "type": "module",
  "author": "Inkverse",
  "license": "AGPL-3.0",
  "description": "Worker Script for Inkverse",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "lodash": "^4.17.21",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
